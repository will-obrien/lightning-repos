<design:component >
    <design:attribute name="buttonId" label="Button ID" description="Live Agent Button Id"/>
    <design:attribute name="deploymentId" label="Deployment ID" description="Live Agent Deployment Id"/>
    <design:attribute name="orgId" label="Organization ID" description="Organization Id"/>
    <design:attribute name="deploymentJsUrl" label="Deployment JS URL" description="Deployment Js URL" default="https://c.la.gus.salesforce.com/content/g/js/37.0/deployment.js"/>
    <design:attribute name="liveagentInitUrl" label="Live Agent Init URL" description="Live Agent Init URL"/>
    <design:attribute name="chatButtonLabel" label="Button Label" description="Live Chat Button Label" default="Live Agent"/>

<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Max_Course_Catalog_Students__c</fullName>
    <externalId>false</externalId>
    <formula>IF
(Number_of_Instructors__c&lt;=0,
Course_Catalog__r.Number_of_Students__c,
Number_of_Instructors__c*Course_Catalog__r.Number_of_Students__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Max number of students based on Course Catalog and Number of Instructors on TE</inlineHelpText>
    <label>Max Course Catalog Students</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>

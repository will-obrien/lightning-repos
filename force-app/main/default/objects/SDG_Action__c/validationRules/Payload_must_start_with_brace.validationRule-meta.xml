<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Payload_must_start_with_brace</fullName>
    <active>true</active>
    <errorConditionFormula>LEFT( Event_Payload__c , 1) != &#39;{&#39;</errorConditionFormula>

<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Fiscal_Month_Numeric__c</fullName>
    <description>Conversion of Calendar month to Fiscal Month</description>
    <externalId>false</externalId>
    <formula>CASE(  MONTH( BATCH_Id__c ) ,
1,7,
2,8,
3,9,
4,10,
5,11,
6,12,
7,1,
8,2,
9,3,
10,4,
11,5,
12,6,
NULL)</formula>
    <label>Fiscal Month Numeric</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>

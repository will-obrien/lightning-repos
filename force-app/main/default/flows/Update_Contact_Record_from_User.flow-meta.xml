                <rightValue>
                    <elementReference>sUser.Street</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>sContact.MailingCity</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>sUser.City</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>sContact.MailingState</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>sUser.State</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>sContact.MailingPostalCode</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>sUser.PostalCode</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>sContact.Title</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>sUser.Title</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>sContact.Phone</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>sUser.Phone</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>sContact.Email</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>sUser.Email</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_User_Record</targetReference>
            </connector>
            <label>Found Record</label>
        </rules>
    </decisions>

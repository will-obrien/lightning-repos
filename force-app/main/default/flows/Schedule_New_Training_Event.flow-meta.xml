                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>sMinOrderItem.ccrz__UnitOfMeasure__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Class</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>sMinOrderItem.ccrz__ExtSKU__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>MINSTUDENT</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Minor_Item_Output</targetReference>
            </connector>
            <label>isStudentorClass</label>
        </rules>
    </decisions>
